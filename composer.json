{
    "name": "localgovdrupal/localgov_page",
    "description": "A page content type for LocalGov Drupal that allows the building of rich content using Paragraphs.",
    "type": "drupal-module",
    "homepage": "https://github.com/localgovdrupal/localgov_page",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "require": {
        "drupal/layout_paragraphs": "^1.0",
        "localgovdrupal/localgov_core": "^2.0",
        "localgovdrupal/localgov_paragraphs": "^2.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "Fix missing link schema in Layout Paragraphs 1.x, see https://www.drupal.org/project/layout_paragraphs/issues/3161998": "https://git.drupalcode.org/issue/layout_paragraphs-3161998/-/commit/e7099de4168f8b65620235bc91e0f04cdfe0ab7c.patch"
        }
    }
}
